{
  "name": "satisfactory-save-editor",
  "displayName": "Satisfactory Save Editor",
  "description": "Edit Satisfactory save files as JSON directly in VS Code.",
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "satisfactory-save-editor.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "10.0.10",
    "@types/node": "24.4.0",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "@vscode/test-cli": "0.0.11",
    "@vscode/test-electron": "2.5.2",
    "eslint": "9.35.0",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@types/pako": "^2.0.4",
    "pako": "^2.1.0",
    "prettier": "3.6.2"
  }
}
